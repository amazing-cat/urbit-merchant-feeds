{
  "$schema" : "http://json-schema.org/draft-04/schema",
  "id" : "schedule.json",
  "description" : "Signal schema generation schedule",
  "title" : "schedule",
  "type": "object",
  "properties": {
    "interval": {
      "type": "string",
      "enum": ["HOURLY", "15MIN", "30MIN", "45MIN","1MIN"],
      "description": "How often is the feed being re-generated by the publisher"
    }
  },
  "required": ["interval"]
}